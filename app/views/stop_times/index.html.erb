<h1>Listing stop_times</h1>

<table>
  <tr>
    <th>Arrival time</th>
    <th>Departure time</th>
    <th>Sequence</th>
    <th>Pickup type</th>
    <th>Drop off type</th>
    <th>Distance traveled</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stop_times.each do |stop_time| %>
  <tr>
    <td><%= stop_time.arrival_time %></td>
    <td><%= stop_time.departure_time %></td>
    <td><%= stop_time.sequence %></td>
    <td><%= stop_time.pickup_type %></td>
    <td><%= stop_time.drop_off_type %></td>
    <td><%= stop_time.distance_traveled %></td>
    <td><%= link_to 'Show', stop_time %></td>
    <td><%= link_to 'Edit', edit_stop_time_path(stop_time) %></td>
    <td><%= link_to 'Destroy', stop_time, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Stop time', new_stop_time_path %>
